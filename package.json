{
  "name": "maquette-typescript-jsx-starter",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "dependencies": {
    "maquette": "3.5.2"
  },
  "devDependencies": {
    "@types/chai-as-promised": "7.1.5",
    "@types/node": "17.0.23",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.8",
    "chai-as-promised": "7.1.1",
    "maquette-jsx": "3.0.1",
    "maquette-query": "0.11.7",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-loader": "9.2.8",
    "typescript-assistant": "0.55.8",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-middleware": "5.3.1",
    "webpack-dev-server": "4.7.4"
  },
  "nyc": {
    "include": [
      "src/**/*.tsx?"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": false,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "scripts": {
    "prepare": "husky install",
    "serve": "npx webpack serve",
    "assist": "tsa",
    "clean": "tsa clean",
    "fixall": "tsa fixall"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "license": "MIT"
}
